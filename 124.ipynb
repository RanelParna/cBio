   "source": [
    "# 6.1 define the scope for testing\n",
    "new_meta_cols_to_test = ['body_site', 'antibiotics_current_use', 'study_condition', 'disease', 'age_category', 'gender', 'country', 'non_westernized', 'sequencing_platform', 'DNA_extraction_kit', 'pregnant', 'family_role', 'born_method', 'feeding_practice', 'location', 'disease_subtype', 'fobt', 'population']\n",
    "MAX_UNIQUE_VALUES_PER_COLUMN_TEST = 5; TOP_N_SUGGESTIONS_DISPLAY = 3\n",
    "# 6.2 execute tests & collect results\n",
    "all_test_results_for_table = []\n",
    "print(f\"Running tests on up to {MAX_UNIQUE_VALUES_PER_COLUMN_TEST} unique values per column...\")\n",
    "for source_col in new_meta_cols_to_test:\n",
    "    if source_col not in new_meta.columns: continue\n",
    "    try: unique_source_values = new_meta[source_col].dropna().astype(str).unique()\n",
    "    except Exception as e: continue\n",
    "    if len(unique_source_values) == 0: continue\n",
    "    for i, input_val in enumerate(unique_source_values):\n",
    "        if i >= MAX_UNIQUE_VALUES_PER_COLUMN_TEST: break\n",
    "        if not input_val.strip(): continue\n",
    "        suggestions, confidence, top_k_vals_raw, pre_boost_score, boost_applied, expected_targets = suggest_attribute_final(\n",
    "            input_value=input_val, source_column_name=source_col, model=model,\n",
    "            target_embeddings=target_value_embeddings, target_value_info=target_value_info,\n",
    "            column_mapping=automated_column_mapping, boost_factor=1.5,\n",
    "            top_n_cols=TOP_N_SUGGESTIONS_DISPLAY, top_n_values=3, agg_k=3 )\n",
    "        result_row = { 'Source Column': source_col, 'Input Value': input_val, 'Suggested Col #1': '-', 'Score #1': np.nan, 'Confidence': 'Low', 'Pre-Boost Score #1': np.nan, 'Boost Applied?': 'No', 'Expected Targets': '(None)', 'Best Single Match #1': '-', 'Top Semantic Matches #1': '-', 'Suggested Col #2': '-', 'Score #2': np.nan, 'Suggested Col #3': '-', 'Score #3': np.nan, 'Curator Status': '', 'Curator Notes': '', }\n",
    "        if suggestions:\n",
    "            result_row['Confidence'] = confidence; sugg1 = suggestions[0]; result_row['Suggested Col #1'] = sugg1[0]\n",
    "            result_row['Score #1'] = sugg1[1]; result_row['Best Single Match #1'] = sugg1[2]\n",
    "            result_row['Pre-Boost Score #1'] = pre_boost_score if pre_boost_score is not None else np.nan\n",
    "            result_row['Boost Applied?'] = \"Yes\" if boost_applied else \"No\"\n",
    "            result_row['Expected Targets'] = \"; \".join(expected_targets) if expected_targets else \"(None)\"\n",
    "            if top_k_vals_raw: result_row['Top Semantic Matches #1'] = \"; \".join([f\"'{val}' ({score:.3f})\" for val, score in top_k_vals_raw[:3]])\n",
    "            else: result_row['Top Semantic Matches #1'] = \"(N/A)\"\n",
    "            if len(suggestions) > 1: sugg2 = suggestions[1]; result_row['Suggested Col #2'] = sugg2[0]; result_row['Score #2'] = sugg2[1]\n",
    "            if len(suggestions) > 2: sugg3 = suggestions[2]; result_row['Suggested Col #3'] = sugg3[0]; result_row['Score #3'] = sugg3[1]\n",
    "        all_test_results_for_table.append(result_row)\n",
    "print(\"Test data collection complete.\")\n",
    "\n",
    "# 6.3 create, sort, style, and display the enhanced results table\n",
    "if all_test_results_for_table:\n",
    "    print(\"\\n--- Preparing Final Results Table for Display ---\")\n",
    "    results_df = pd.DataFrame(all_test_results_for_table)\n",
    "    score_cols = ['Score #1', 'Pre-Boost Score #1', 'Score #2', 'Score #3']\n",
    "    for col in score_cols:\n",
    "        if col in results_df.columns: results_df[col] = pd.to_numeric(results_df[col], errors='coerce')\n",
    "    confidence_order = ['Low', 'Medium', 'High']\n",
    "    if 'Confidence' in results_df.columns:\n",
    "         results_df['Confidence'] = pd.Categorical(results_df['Confidence'], categories=confidence_order, ordered=True)\n",
    "         results_df = results_df.sort_values(by=['Confidence', 'Score #1'], ascending=[True, True], na_position='first')\n",
    "    def style_confidence(s):\n",
    "        style = 'font-weight: bold;';\n",
    "        if s == 'Low': style += ' background-color: #FFF0F0;' # light red/pink\n",
    "        return style\n",
    "    # styling\n",
    "    styled_df = results_df.style\\\n",
    "        .map(style_confidence, subset=['Confidence'])\\\n",
    "        .background_gradient(cmap='Greens', subset=[col for col in score_cols if col in results_df.columns], vmin=0.5, vmax=1.0) \\\n",
    "        .set_properties(**{'text-align': 'left'}) \\\n",
    "        .format({col: '{:.3f}' for col in score_cols if col in results_df.columns}, na_rep='-') # format existing score cols\n",
    "    # display settings\n",
    "    pd.set_option('display.max_rows', None); pd.set_option('display.max_columns', None)\n",
    "    pd.set_option('display.width', 2000); pd.set_option('display.max_colwidth', 70)\n",
    "    print(\"\\n--- Curated Suggestion Results Table (Sorted & Styled) ---\")\n",
    "    display(styled_df)\n",
    "else:\n",
    "    print(\"\\nNo test results were generated to display.\")"
   ]
