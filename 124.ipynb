    "# helper function for column stats\n",
    "def get_col_stats(df, col_name):\n",
    "    \"\"\"Calculates summary statistics for a given column.\"\"\"\n",
    "    if col_name not in df.columns:\n",
    "         return None\n",
    "    series = df[col_name]\n",
    "    total_rows = len(df)\n",
    "    missing_count = series.isnull().sum()\n",
    "    missing_percent = (missing_count / total_rows) * 100 if total_rows > 0 else 0\n",
    "    unique_values = series.dropna().unique()\n",
    "    num_unique = len(unique_values)\n",
    "    sample_size = 2\n",
    "    unique_sample = list(unique_values[:sample_size])\n",
    "    ellipsis = \" ...\" if num_unique > sample_size else \"\"\n",
    "    sample_str = f\"{unique_sample}{ellipsis}\"\n",
    "\n",
    "    return {\n",
    "        'Name': col_name,\n",
    "        'Dtype': str(series.dtype),\n",
    "        'Missing %': f\"{missing_percent:.2f}\",\n",
    "        'Unique Count': num_unique,\n",
    "        'Sample Values': sample_str\n",
    "    }\n",
    "\n",
    "# prep mappings and master list\n",
    "new_name_map = {col.lower(): col for col in new_meta.columns}\n",
    "curated_name_map = {col.lower(): col for col in curated_meta.columns}\n",
    "all_cols_lower = set(new_name_map.keys()) | set(curated_name_map.keys())\n",
    "matching_cols_lower = set(new_name_map.keys()) & set(curated_name_map.keys())\n",
    "\n",
    "# build table data\n",
    "summary_data = []\n",
    "for col_lower in sorted(list(all_cols_lower)):\n",
    "    is_match = col_lower in matching_cols_lower\n",
    "    new_orig_name = new_name_map.get(col_lower)\n",
    "    curated_orig_name = curated_name_map.get(col_lower)\n",
    "\n",
    "    new_stats = get_col_stats(new_meta, new_orig_name)\n",
    "    curated_stats = get_col_stats(curated_meta, curated_orig_name)\n",
    "\n",
    "    row_data = {\n",
    "        'Column (Lowercase)': col_lower,\n",
    "        'Match?': '✅ Yes' if is_match else '➖ No',\n",
    "        'New Name': new_stats['Name'] if new_stats else '-',\n",
    "        'New Dtype': new_stats['Dtype'] if new_stats else '-',\n",
    "        'New Missing %': new_stats['Missing %'] if new_stats else '-',\n",
    "        'New Unique #': new_stats['Unique Count'] if new_stats else '-',\n",
    "        'New Sample': new_stats['Sample Values'] if new_stats else '-',\n",
    "        'Curated Name': curated_stats['Name'] if curated_stats else '-',\n",
    "        'Curated Dtype': curated_stats['Dtype'] if curated_stats else '-',\n",
    "        'Curated Missing %': curated_stats['Missing %'] if curated_stats else '-',\n",
    "        'Curated Unique #': curated_stats['Unique Count'] if curated_stats else '-',\n",
    "        'Curated Sample': curated_stats['Sample Values'] if curated_stats else '-',\n",
    "    }\n",
    "    summary_data.append(row_data)\n",
    "\n",
    "# create df\n",
    "summary_df = pd.DataFrame(summary_data)\n",
    "summary_df = summary_df.set_index('Column (Lowercase)')\n",
    "\n",
    "# final row ordering:\n",
    "# 1. columns from new_meta; original order.\n",
    "# 2. columns present ONLY in curated_meta; original order.\n",
    "new_meta_lower_order = [col.lower() for col in new_meta.columns]\n",
    "curated_meta_lower_order = [col.lower() for col in curated_meta.columns]\n",
    "new_meta_lower_set = set(new_meta_lower_order)\n",
    "curated_only_lower_in_original_order = [\n",
    "    col_lower for col_lower in curated_meta_lower_order\n",
    "    if col_lower not in new_meta_lower_set\n",
    "]\n",
    "final_order = new_meta_lower_order + curated_only_lower_in_original_order\n",
    "valid_final_order = [col for col in final_order if col in summary_df.index]\n",
    "\n",
    "# reindex df\n",
    "summary_df_reordered = summary_df.reindex(valid_final_order)\n",
    "\n",
    "# avoid truncation\n",
    "pd.set_option('display.max_rows', None)\n",
    "pd.set_option('display.max_columns', None)\n",
    "display(summary_df_reordered)"
   ]
